{
  "name" : "RockeTab",
  "author" : "Nikhil Netaji Ingale",
  "description" : "An inspirational quote on every new tab",
  "version" : "1.0.0",
  "manifest_version" : 3,

  "chrome_url_overrides" : {
    "newtab" : "index.html"
  },

  "action" : {
    "default_popup": "popup.html",
    "default_title": "RockeTab"
  },

  "icons" : {
    "16" : "icon16.png",
    "32" : "icon32.png",
    "64" : "icon64.png",
    "128" : "icon128.png"
  },

  "offline_enabled" : true,

  "permissions" : ["activeTab"]
}
